{
  "name": "final-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:unit": "vitest --run tests/unit",
    "test:integration": "vitest --run tests/integration",
    "test:all": "npm run test && npm run test:bdd",
    "test:watch": "vitest --watch",
        "test:bdd": "start-server-and-test dev http://localhost:5173 \"cucumber-js -c cucumber.cjs\"",
    "test:bdd:headed": "cross-env HEADED=1 start-server-and-test dev http://localhost:5173 \"cucumber-js -c cucumber.cjs\"",
    "test:bdd:tags": "start-server-and-test dev http://localhost:5173 \"cucumber-js -c cucumber.cjs --tags @smoke\"",
    "allure:generate": "allure generate allure-results --clean",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results",
        "test:bdd:allure": "echo \"Allure for Cucumber not configured. Install allure-cucumberjs and set formatter.\" && exit 0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/vite": "^4.1.4",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "allure-playwright": "^3.3.2",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "playwright-bdd": "^8.3.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.2.4"
  }
}
